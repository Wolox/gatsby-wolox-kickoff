{
  "name": "gatsby-landing-page-starter",
  "private": true,
  "description": "A simple landing page starter template with Gatsby",
  "version": "1.0.0",
  "author": "Bilal Tahir",
  "dependencies": {
    "gatsby": "^2.21.22",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-manifest": "^2.4.2",
    "gatsby-plugin-offline": "^3.2.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sass": "^2.3.1",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-plugin-sitemap": "^2.4.2",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-sharp": "^2.5.2",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "eslint-config-wolox": "4.0.0",
    "eslint-config-wolox-react": "2.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "4.0.5",
    "gatsby-plugin-root-import": "^2.0.5",
    "husky": "^4.2.5",
    "prettier": "1.18.2",
    "stylelint": "^13.6.1",
    "stylelint-config-wolox": "^1.0.9",
    "stylelint-no-indistinguishable-colors": "^1.3.0",
    "stylelint-scss": "^3.18.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-diff && npm run lint-scss && CI=true npm run test"
    }
  },
  "keywords": [
    "gatsby",
    "react",
    "landing"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.tsx,.ts src",
    "lint-fix": "./node_modules/eslint/bin/eslint.js --ext .js,.tsx,.ts src --fix",
    "lint-scss": "./node_modules/stylelint/bin/stylelint.js '**/*.scss'",
    "lint-diff": "git diff --name-only --cached --relative --diff-filter=ACM | grep -e \\.js$ -e \\.tsx$ -e \\.ts$ | xargs eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/btahir/gatsby-landing-page-starter"
  },
  "bugs": {
    "url": "https://github.com/btahir/gatsby-landing-page-starter/issues"
  }
}
