{
  "name": "gatsby-starter-landing-page",
  "private": true,
  "description": "A simple starter to create a minimal landing page with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Gill <kyle.robert.gill@gmail.com>",
  "dependencies": {
    "gatsby": "^2.1.37",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-sass": "^2.3.7",
    "gatsby-plugin-sharp": "^2.0.29",
    "gatsby-plugin-sitemap": "^2.0.10",
    "gatsby-source-filesystem": "^2.0.27",
    "gatsby-transformer-sharp": "^2.1.17",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "sharp": "^0.22.0"
  },
  "devDependencies": {
    "eslint-config-wolox": "4.0.0",
    "eslint-config-wolox-react": "2.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "4.0.5",
    "gatsby-plugin-root-import": "^2.0.5",
    "prettier": "1.18.2",
    "stylelint": "^13.6.1",
    "stylelint-config-wolox": "^1.0.9",
    "stylelint-no-indistinguishable-colors": "^1.3.0",
    "stylelint-scss": "^3.18.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-diff && npm run lint-scss && CI=true npm run test"
    }
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.tsx,.ts src",
    "lint-fix": "./node_modules/eslint/bin/eslint.js --ext .js,.tsx,.ts src --fix",
    "lint-scss": "./node_modules/stylelint/bin/stylelint.js '**/*.scss'",
    "lint-diff": "git diff --name-only --cached --relative --diff-filter=ACM | grep -e \\.js$ -e \\.tsx$ -e \\.ts$ | xargs eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
